{
  "name": "webpack-mock-server",
  "version": "1.0.14",
  "description": "Mocks api requests for webpack-dev-server with hot-replacement",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "npm run lint",
    "lint": "npm run lint-code && npm run lint-md",
    "lint-fix": "npm run lint-code -- --fix && npm run lint-md -- --fix",
    "lint-code": "./node_modules/.bin/eslint ./**/*.{js,jsx,ts,tsx,json}",
    "lint-md": "./node_modules/.bin/markdownlint **/*.md --ignore node_modules",
    "serve-node": "npm run build && node ./test/start",
    "serve": "npm run build && npm run run-webpack",
    "run-webpack": "./node_modules/.bin/webpack serve --hot --config test/webpack.config.js",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yegorich555/webpack-mock-server.git"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "keywords": [
    "webpack",
    "mock",
    "mocker",
    "api",
    "webpack-dev-server",
    "typescript",
    "ts",
    "js",
    "json",
    "hot",
    "replacement",
    "hmr",
    "expressjs",
    "middleware"
  ],
  "author": "yegorich555",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Yegorich555/webpack-mock-server/issues"
  },
  "homepage": "https://github.com/Yegorich555/webpack-mock-server#readme",
  "devDependencies": {
    "@types/multer": "^1.4.7",
    "@types/node": "^13.13.52",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-prettier": "^3.4.0",
    "express": "^4.17.1",
    "prettier": "^1.19.1",
    "typescript": "^3.9.10",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "multer": "^1.4.2"
  },
  "peerDependencies": {
    "typescript": ">=2.7",
    "express": "*"
  },
  "files": [
    "lib/*",
    "public/*"
  ],
  "publishConfig": {
    "save-prefix": "^",
    "save-dev": true
  }
}
