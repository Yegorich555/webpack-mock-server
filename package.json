{
  "name": "webpack-mock-server",
  "version": "1.0.19",
  "description": "Mocks api requests for webpack-dev-server with hot-replacement",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "npm run lint",
    "lint": "npm run lint-code && npm run lint-md",
    "lint-code": ".\\node_modules\\.bin\\eslint ./**/*.{js,jsx,ts,tsx,json} --fix",
    "lint-md": ".\\node_modules\\.bin\\markdownlint **/*.md --ignore node_modules --fix",
    "serve-node": "npm run build && node .\\test\\start",
    "serve": "npm run build && npm run run-webpack",
    "run-webpack": ".\\node_modules\\.bin\\webpack serve --hot --config test/webpack.config.js",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yegorich555/webpack-mock-server.git"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "keywords": [
    "webpack",
    "mock",
    "mocker",
    "api",
    "webpack-dev-server",
    "typescript",
    "ts",
    "js",
    "json",
    "hot",
    "replacement",
    "hmr",
    "express",
    "middleware"
  ],
  "author": "Yegorich555",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Yegorich555/webpack-mock-server/issues"
  },
  "homepage": "https://github.com/Yegorich555/webpack-mock-server#readme",
  "devDependencies": {
    "@types/multer": "^1.4.7",
    "@types/node": "^18.16.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.2",
    "markdownlint-cli": "^0.33.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2",
    "webpack-dev-server": "^4.13.3"
  },
  "dependencies": {
    "@types/express": "^4.17.17",
    "multer": "^1.4.5-lts.1"
  },
  "peerDependencies": {
    "typescript": ">=2.7",
    "express": "*",
    "@types/multer": "*"
  },
  "files": [
    "lib/*",
    "public/*"
  ]
}
