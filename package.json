{
  "name": "webpack-mock-server",
  "version": "1.0.17",
  "description": "Mocks api requests for webpack-dev-server with hot-replacement",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "npm run lint",
    "lint": "npm run lint-code && npm run lint-md",
    "lint-code": "./node_modules/.bin/eslint ./**/*.{js,jsx,ts,tsx,json} --fix",
    "lint-md": "./node_modules/.bin/markdownlint **/*.md --ignore node_modules --fix",
    "serve-node": "npm run build && node ./test/start",
    "serve": "npm run build && npm run run-webpack",
    "run-webpack": "./node_modules/.bin/webpack serve --hot --config test/webpack.config.js",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yegorich555/webpack-mock-server.git"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "keywords": [
    "webpack",
    "mock",
    "mocker",
    "api",
    "webpack-dev-server",
    "typescript",
    "ts",
    "js",
    "json",
    "hot",
    "replacement",
    "hmr",
    "expressjs",
    "middleware"
  ],
  "author": "yegorich555",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Yegorich555/webpack-mock-server/issues"
  },
  "homepage": "https://github.com/Yegorich555/webpack-mock-server#readme",
  "devDependencies": {
    "@types/multer": "^1.4.7",
    "@types/node": "^18.0.1",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.2.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.1",
    "markdownlint-cli": "^0.31.1",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "multer": "^1.4.5-lts.1"
  },
  "peerDependencies": {
    "typescript": ">=2.7",
    "express": "*",
    "@types/multer": "*"
  },
  "files": [
    "lib/*",
    "public/*"
  ],
  "publishConfig": {
    "save-prefix": "^",
    "save-dev": true
  }
}
